<launch>
    <arg name="imu_input_topic" default="/sensing/ins/starneto/imu/raw_data" />
    <arg name="imu_output_topic" default="/sensing/imu/imu_data" />

    <include file="$(find-pkg-share imu_corrector)/launch/imu_corrector.launch.xml">
        <arg name="input_topic" value="$(var imu_input_topic)"/>
        <arg name="output_topic" value="$(var imu_output_topic)"/>
        <arg name="param_file" value="$(find-pkg-share nightowl_launch)/config/sensor/imu_corrector.param.yaml"/>
    </include>
</launch>
